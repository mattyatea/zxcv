/**
 * Staging environment configuration for Cloudflare Workers
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "zxcv-backend-and-frontend-staging",
	"main": "./.output/server/index.mjs",
	"compatibility_date": "2025-07-15",
	"observability": {
		"enabled": true
	},
	"compatibility_flags": ["nodejs_compat"],
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "zxcv-db-staging",
			"database_id": "staging-database-id"
		}
	],
	"r2_buckets": [
		{
			"binding": "R2",
			"bucket_name": "zxcv-staging"
		}
	],
	"send_email": [
		{
			"name": "EMAIL_SENDER",
			"destination_address": "noreply@staging.zxcv.dev"
		}
	],
	"vars": {
		"JWT_ALGORITHM": "HS256",
		"JWT_EXPIRES_IN": "7d",
		"RATE_LIMIT_ANONYMOUS": "100",
		"RATE_LIMIT_AUTHENTICATED": "1000",
		"RATE_LIMIT_API_KEY": "5000",
		"EMAIL_FROM": "noreply@staging.zxcv.dev",
		"FRONTEND_URL": "https://staging.zxcv.dev"
	}
}