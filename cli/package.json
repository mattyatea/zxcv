{
	"name": "cli",
	"version": "0.1.0",
	"description": "AI coding rules management CLI tool",
	"main": "dist/index.js",
	"bin": {
		"zxcv": "./dist/index.js"
	},
	"type": "module",
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun build src/index.ts --compile --outfile dist/zxcv",
		"build:cross": "bun run scripts/build-cross-platform.ts",
		"lint": "biome check --write",
		"format": "biome format --write",
		"typecheck": "tsc --noEmit",
		"check": "biome check"
	},
	"keywords": ["cli", "ai", "rules", "management"],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.11.0",
		"chalk": "^5.4.1",
		"commander": "^12.1.0",
		"inquirer": "^9.3.7",
		"open": "^10.2.0",
		"ora": "^8.2.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"@types/inquirer": "^9.0.8",
		"@types/node": "^20.19.9",
		"typescript": "^5.8.3"
	}
}
